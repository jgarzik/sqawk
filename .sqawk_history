#V2
.help
.tables
SELECT * FROM sample;
.exit
SELECT
  id,
  name,
  age
FROM
  sample
WHERE
  age > 25;
.exit
.tables
SELECT COUNT(*) FROM sample;
UPDATE sample SET age = 31 WHERE id = 1;
SELECT * FROM sample;
.exit
.write
UPDATE sample SET age = 32 WHERE id = 1;
.write
SELECT * FROM sample;
.exit
